<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <clear />
        <add name="X-Frame-Options" value="DENY" />
        <add name="X-Xss-Protection" value="1; mode=block" />
        <add name="X-Content-Type-Options" value="nosniff" />
        <remove name="X-Powered-By" />
      </customHeaders>
      <redirectHeaders>
        <clear />
      </redirectHeaders>
    </httpProtocol>
    <staticContent>
      <remove fileExtension=".woff" />
      <remove fileExtension=".woff2" />
      <remove fileExtension=".ttf" />
      <remove fileExtension=".json" />
      <remove fileExtension=".xml" />
      <remove fileExtension=".jpg" />
      <remove fileExtension=".png" />
      <remove fileExtension=".gif" />
      <mimeMap fileExtension=".woff" mimeType="application/font-woff" />
      <mimeMap fileExtension=".woff2" mimeType="application/font-woff2" />
      <mimeMap fileExtension=".ttf" mimeType="application/x-font-ttf" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
      <mimeMap fileExtension=".xml" mimeType="application/xml" />
      <mimeMap fileExtension=".jpg" mimeType="image/jpeg" />
      <mimeMap fileExtension=".png" mimeType="image/png" />
      <mimeMap fileExtension=".gif" mimeType="image/gif" />
    </staticContent>
    <rewrite>
      <rules>
        <rule name="AkamaiVvtkWWW" stopProcessing="true">
          <match url="^.well-known/acme-challenge/zNnXWt7nMPfP23nGy9wIbhSXZsHu8Q4ztvTExN6b5Co$" ignoreCase="true" />
          <action type="Redirect" url="http://dcv.akamai.com/.well-known/acme-challenge/zNnXWt7nMPfP23nGy9wIbhSXZsHu8Q4ztvTExN6b5Co" redirectType="Permanent" />
        </rule>

        <rule name="AkamaiVvtk" stopProcessing="true">
          <match url="^.well-known/acme-challenge/RCwCl9ozYmzsvJz7x35QZQCiE24ajrLBTjPMV-JF-f0$" ignoreCase="true" />
          <action type="Redirect" url="http://dcv.akamai.com/.well-known/acme-challenge/RCwCl9ozYmzsvJz7x35QZQCiE24ajrLBTjPMV-JF-f0" redirectType="Permanent" />
        </rule>

        <rule name="AkamaiProduction" stopProcessing="true">
          <match url="^.well-known/acme-challenge/Wq8drCx0HlF21xLWm5TDicHvQcNgcXUl35nV1pAFGlQ$" ignoreCase="true" />
          <action type="Redirect" url="http://dcv.akamai.com/.well-known/acme-challenge/Wq8drCx0HlF21xLWm5TDicHvQcNgcXUl35nV1pAFGlQ" redirectType="Permanent" />
        </rule>

        <rule name="SPA" stopProcessing="true">
          <match url=".*" />
          <action type="Rewrite" url="/" />
          <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
            <add input="{REQUEST_FILENAME}" matchType="IsDirectory" negate="true" />
          </conditions>
        </rule>
      </rules>
    </rewrite>
    <security>
      <requestFiltering removeServerHeader="true">
      </requestFiltering>
    </security>
  </system.webServer>
</configuration>
